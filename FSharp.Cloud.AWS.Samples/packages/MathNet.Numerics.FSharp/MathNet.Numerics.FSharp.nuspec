      <dependency id="MathNet.Numerics" version="[3.5.0]" />
      <dependency id="FSharp.Core" version="3.1.2.1" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Numerics" targetFramework=".NETFramework4.0, Windows8.0, Silverlight5.0, MonoAndroid1.0, MonoTouch1.0" />
    </frameworkAssemblies>
